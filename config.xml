<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.radionorth" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Radionorth</name>
    <description>
        Aplicativo para pacientes e dentistas da Radionorth acessarem seus exames, laudos e toda sua documentação odontológica
    </description>
    <author email="ivancordeiro@yahoo.com.br" href="http://www.ivanprogramador.com.br">
        Ivan Programador
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
